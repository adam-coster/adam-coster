{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"check": {
			"cache": false
		},
		"build": {
			"dependsOn": ["^build"],
			"cache": false,
			"outputs": [
				"dist/**",
				"out/**",
				"build/**",
				"app/**",
				".netlify/**",
				".svelte-kit/**"
			]
		},
		"watch": {
			"dependsOn": ["build"],
			"outputs": [],
			"cache": false
		},
		"test": {
			"dependsOn": ["build"],
			"outputs": []
		}
	},
	"globalDependencies": [
		".env",
		"tsconfig.json",
		"$GITHUB_TOKEN",
		"package.json",
		"package-lock.json"
	]
}
