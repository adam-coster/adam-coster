		"watch": "tsc --build --watch"
	},
	"dependencies": {
		"@bscotch/utility": "6.8.0",
		"fs-extra": "^11.1.0",
		"open": "^8.4.0",
		"win-audio": "^2.0.2"
	},
	"devDependencies": {
		"@types/fs-extra": "^11.0.1",
		"@types/node": "18.11.18",
		"@typescript-eslint/eslint-plugin": "5.49.0",
		"@typescript-eslint/parser": "5.49.0",
		"eslint": "8.32.0",
		"typescript": "4.9.4"
	}
}
