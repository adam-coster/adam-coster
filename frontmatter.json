  "frontMatter.content.pageFolders": [
    {
      "path": "[[workspace]]/projects/site/src/routes/blog",
      "title": "Blog Posts"
    }
  ],
  "frontMatter.taxonomy.slugPrefix": null,
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fileType": "md",
      "fields": [
        {
          "title": "slug",
          "name": "slug",
          "type": "string"
        },
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "editedAt",
          "name": "editedAt",
          "type": "datetime",
          "isModifiedDate": true
        },
        {
          "title": "publishedAt",
          "name": "publishedAt",
          "type": "datetime",
          "isPublishDate": true
        },
        {
          "title": "canonical",
          "name": "canonical",
          "type": "string"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "crossPosts",
          "name": "crossPosts",
          "type": "string",
          "multiple": true
        },
        {
          "title": "images",
          "name": "images",
          "type": "string",
          "multiple": true
        }
      ]
    }
  ],
  "frontMatter.content.publicFolder": "projects/site/static"
}
